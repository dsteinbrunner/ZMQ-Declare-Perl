{
  "ventilator":{
    "type": "mypipeline_ventilator",
    "sender":{
      "type": "push",
      "bind": "tcp://*:5557"
    }
  },

  "worker":{
    "type": "mypipeline_worker",
    "input":{
      "type":    "pull",
      "connect": "tcp://localhost:5557"
    },
    "output":{
      "type":    "push",
      "connect": "tcp://localhost:5558"
    }
  },

  "sink":{
    "type": "mypipeline_sink",
    "receiver":{
      "type": "pull",
      "bind": "tcp://*:5558"
    }
  }
}
